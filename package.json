{
  "name": "footballwithfriends",
  "author": "Jose Manuel Dur√°n",
  "version": "1.0.0",
  "private": true,
  "description": "An Ionic project to manage football games with friends",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/josemanuelduran/football-with-friends-client.git"
  },
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "build:android": "ionic cordova build android",
    "serve": "ionic serve",
    "serve:android": "ionic serve --platform android",
    "run:android": "ionic cordova run android",
    "test": "ng test",
    "test:ci": "ng test --single-run --progress false --browsers ChromeNoSandbox --reporters progress,junit",
    "test:coverage": "ng test --single-run --progress false --code-coverage --browsers ChromeNoSandbox",
    "lint": "ng lint",
    "e2e": "protractor",
    "e2e:prerrequisites": "webdriver-manager update",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve"
  },
  "dependencies": {
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@ionic-native/core": "4.6.0",
    "@ionic-native/in-app-browser": "4.6.0",
    "@ionic-native/keyboard": "4.6.0",
    "@ionic-native/splash-screen": "4.6.0",
    "@ionic-native/status-bar": "4.6.0",
    "@ionic-native/vibration": "4.6.0",
    "@ionic/storage": "2.1.3",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "5.2.0",
    "@ngrx/store": "5.2.0",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "3.0.1",
    "angular2-jwt": "^0.2.3",
    "angular2-moment": "^1.8.0",
    "cordova-android": "6.2.3",
    "cordova-ios": "4.5.2",
    "cordova-plugin-crosswalk-webview": "^2.3.0",
    "cordova-plugin-device": "^1.1.6",
    "cordova-plugin-inappbrowser": "^1.7.1",
    "cordova-plugin-ionic-keyboard": "^2.0.2",
    "cordova-plugin-ionic-webview": "^1.1.15",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-statusbar": "^2.2.3",
    "cordova-plugin-whitelist": "^1.3.2",
    "express": "^4.16.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "moment": "2.22.0",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@ionic/app-scripts": "3.1.6",
    "@types/jasmine": "2.5.47",
    "@types/lodash": "4.14.62",
    "codelyzer": "4.2.1",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.3",
    "ngrx-store-freeze": "^0.2.1",
    "typescript": "2.6.2"
  },
  "config": {
    "ionic_aot_write_to_disk": true
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "22+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-device": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-ionic-keyboard": {}
    }
  }
}